{
  "name": "graphql-aggregator-service",
  "version": "1.0.0",
  "description": "A GraphQL aggregator service for GitHub and GitLab APIs.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "build": " npm run format && npm run lint --fix && tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix"
  },
  "dependencies": {
    "@graphql-tools/utils": "^10.8.6",
    "apollo-server": "^3.6.7",
    "axios": "^1.8.4",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^9.0.18",
    "newrelic": "^12.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/jest": "^27.0.2",
    "@types/newrelic": "^9.14.8",
    "@types/node": "^16.11.7",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "jest": "^27.4.5",
    "prettier": "^3.5.3",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "typescript-eslint": "^8.30.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
